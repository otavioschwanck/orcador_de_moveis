<%- model_class = UnregistredItem -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:description) %>:</strong></dt>
  <dd><%= @unregistred_item.description %></dd>
  <dt><strong><%= model_class.human_attribute_name(:value) %>:</strong></dt>
  <dd><%= @unregistred_item.value %></dd>
  <dt><strong><%= model_class.human_attribute_name(:qtd) %>:</strong></dt>
  <dd><%= @unregistred_item.qtd %></dd>
  <dt><strong><%= model_class.human_attribute_name(:mobile_id) %>:</strong></dt>
  <dd><%= @unregistred_item.mobile_id %></dd>
</dl>



<%= link_to t('.back', :default => t("helpers.links.back")),
              unregistred_items_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_unregistred_item_path(@unregistred_item), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              unregistred_item_path(@unregistred_item),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
