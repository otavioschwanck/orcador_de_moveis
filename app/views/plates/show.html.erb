<%- model_class = Plate -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:width) %>:</strong></dt>
  <dd><%= @plate.width %></dd>
  <dt><strong><%= model_class.human_attribute_name(:height) %>:</strong></dt>
  <dd><%= @plate.height %></dd>
  <dt><strong><%= model_class.human_attribute_name(:plate_value) %>:</strong></dt>
  <dd><%= @plate.plate_value %></dd>
  <dt><strong><%= model_class.human_attribute_name(:qtd) %>:</strong></dt>
  <dd><%= @plate.qtd %></dd>
  <dt><strong><%= model_class.human_attribute_name(:mobile_id) %>:</strong></dt>
  <dd><%= @plate.mobile_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              plates_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_plate_path(@plate), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              plate_path(@plate),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
